                    <li class="nav-item"><a asp-controller="Patients" asp-action="Index" class="nav-link"  style="color: rgba(75,74,74,0.8);"><i class="fas fa-user" style="color: #ffffff;"></i><span style="padding: 11px;color: #ffffff;">Patients</span></a></li>
                    <li class="nav-item"><a asp-controller="Appointmentq" asp-action="Index" class="nav-link"  style="color: rgba(75,74,74,0.8);"><i class="fas fa-user" style="color: #ffffff;"></i><span style="padding: 11px;color: #ffffff;">Appointment Queue</span></a></li>
